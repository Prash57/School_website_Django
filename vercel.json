{
    "version": 2,
    "build": [
        {
            "src": "school/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxambdaSize": "15mb", "runtime": "python3.9"}
            },
            {
                "src": "build_files.sh",
                "use": "@vercel/static-build",
                "config": {
                    "distDir": "staticfiles_build"
                }
            }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "school/wsgi.py"
        }
    ]
}
